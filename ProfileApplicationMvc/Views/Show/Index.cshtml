
@{
    var p = ViewBag.bag;
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link href="~/Css/bootstrap.min.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div class="container">
        @if(p!=null)
        {
            <table>
                <tr>

                    <th>ID</th>
                    <th>Name</th>
                    <th>Address</th>
                    <th>Contact Number</th>
                    <th>Salary</th>
                    <th>City</th>
                    <th>Gender</th>
                    <th>Department</th>
                    <th>Company</th>

                </tr>

                @foreach(var str in p)
                {
            <tr>

                <td>@str.empid</td>
                <td>@str.Name</td>
                <td>@str.Addr</td>
                <td>@str.phno</td>
                <td>@str.Sal</td>
                <td>@str.City</td>
                <td>@str.Gen</td>
                <td>@str.Dept</td>
                <td>@str.Comp</td>


            </tr>
                    
                }

            </table>
        }
    </div>
</body>
</html>
